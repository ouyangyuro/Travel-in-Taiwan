{
  "version": 2,
  "builds": [
    {
      "src": ".storybook/**",
      "use": "@vercel/static"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/static"
    },
    {
      "src": "out/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/out/$1"
    }
  ]
}
